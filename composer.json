{
  "name": "cobweb/svconnector_csv",
  "type": "typo3-cms-extension",
  "description": "Connector service for reading a CSV or similar flat file",
  "license": [
    "GPL-2.0-or-later"
  ],
  "authors": [
    {
      "name": "Francois Suter",
      "role": "Developer",
      "homepage": "https://www.ideative.ch/en/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cobweb\\SvconnectorCsv\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "^11.5 || ^12.4",
    "php": "^7.4 || ^8.0 || ^8.1",
    "cobweb/svconnector": "^5.0"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "svconnector_csv"
    }
  },
  "replace": {
    "typo3-ter/svconnector_csv": "self.version"
  }
}
